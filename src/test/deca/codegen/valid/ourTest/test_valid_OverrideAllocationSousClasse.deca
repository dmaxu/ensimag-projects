// Test avancée : allocation mémoire +sous classe + override
// Resultats:
// 0 10 0

class A{
    int x;

    void setX(int g){
        this.x = g;
        println(g, " ", this.x);
    }
}

class B extends A{
    int h;
    void setX(int h){
        this.h = h;
    }
    void afficheH(){
        println(h);
    }
}

{
    B b = new B();
    A a = new B();
    b.afficheH();
    b.setX(10);
    b.afficheH();
    a.setX(10);
    println(a.x);
}
